#page-preloader {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: #59c5ff;
  z-index: 100500;
}

#page-preloader .spinner {
  width: 41px;
  height: 41px;
  position: absolute;
  left: 50%;
  top: 50%;
  background: url('../../../../public/images/pufff.svg') no-repeat 50% 50%;
  margin: -16px 0 0 -16px;
}