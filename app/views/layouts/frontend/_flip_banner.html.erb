<!-- Flip banner -->
<a href="<%= residential_path %>"
   class="flip-banner parallax"
   data-background="/images/flip-banner-bg.jpeg"
   data-color="#274abb"
   data-color-opacity="0.9"
   data-img-width="2500"
   data-img-height="1600">
  <div class="flip-banner-content">
    <h2 class="flip-visible"><%= title_visible %></h2>
    <h2 class="flip-hidden"><%= title_hidden %> <i class="sl sl-icon-arrow-right"></i></h2>
  </div>
</a>
<!-- Flip banner / End -->