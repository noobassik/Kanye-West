<%= form_with model: @object, data: { remote: true, type: :json }, html: { 'data-ajax-submit' => '' } do |f| %>
  <ul class="nav nav-tabs" role="tablist">
    <li role="presentation" class="active"><a href="#general" aria-controls="general" role="tab" data-toggle="tab">General</a></li>
    <li role="presentation"><a href="#seo" aria-controls="seo" role="tab" data-toggle="tab">Seo Page</a></li>
  </ul>

  <!-- Tab panes -->
  <div class="tab-content">
    <div role="tabpanel" class="tab-pane active" id="general">
      <%= render partial: "admin/article_categories/form/general", locals: { f: f } %>
    </div>

    <div role="tabpanel" class="tab-pane" id="seo">
      <%= render partial: "admin/article_categories/form/seo", locals: { f: f, template_fieldset: @object.seo_articles_page.default_page } %>
    </div>
  </div>

  <%= render_control_buttons(form: f,
                             obj: @object,
                             confirm_message: t(:delete_confirm, scope: :article_category)) %>
<% end %>