<%- if controller_name != 'sessions' %>
  <%= link_to t(:log_in, scope: :devise) , new_session_path(resource_name) %><br>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to t(:sign_up, scope: :devise) , new_registration_path(resource_name) %><br>
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to t(:forgot_password, scope: :devise) , new_password_path(resource_name) %><br>
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to t(:not_recive_confirm, scope: :devise) , new_confirmation_path(resource_name) %><br>
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to t(:not_recive_unlock, scope: :devise), new_unlock_path(resource_name) %><br>
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to t(:sing_in_with, scope: :devise) {OmniAuth::Utils.camelize(provider)}, omniauth_authorize_path(resource_name, provider) %><br>
  <% end -%>
<% end -%>
